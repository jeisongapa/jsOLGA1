          "_sourceTheme": "gamem5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer1 gamem5\" group=\"Footers\" global once=\"footers\" mbr-class=\"{'mbr-fullscreen': fullScreen,'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" checked>\n        <input type=\"checkbox\" title=\"Nav\" name=\"showNav\">\n        <input type=\"color\" title=\"Active Color\" value=\"#d91717\" name=\"active\" condition=\"showNav\" selected>\n        <input type=\"checkbox\" title=\"Social\" name=\"showSoc\">\n        <input type=\"range\" inline title=\"Social Amount\" name=\"soc\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showSoc\">\n        <input type=\"color\" title=\"Social Color\" value=\"#ffffff\" name=\"socColor\" condition=\"showSoc\" selected>\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopy\" checked>\n        <input type=\"checkbox\" title=\"Border\" name=\"showBorder\" checked>\n        <input type=\"color\" title=\"Border Color\" value=\"#d91717\" name=\"border\" condition=\"showBorder\" selected>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/gamem5/components/_images/background1.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#000000\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/gamem5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\">\n                <div class=\"title-wrapper\" mbr-if=\"showTitle || showLogo\">\n                    <div class=\"title-wrap\">\n                        <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .title-wrapper\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\">\n                            Templo</h2>\n                        <div class=\"logo-wrap\" mbr-if=\"showLogo\">\n                            <img src=\"@PROJECT_PATH@/assets/images/logo-500x500.png\" alt=\"Mobirise\">\n                        </div>\n                        <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .title-wrapper\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\">Sagrado</h2>\n                    </div>\n                </div>\n                <div class=\"nav-wrapper\" mbr-if=\"showNav\">\n                    <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".list, .nav-wrapper, .soc-wrapper\" data-multiline mbr-article>\n                        <li class=\"item-wrap\">HOME</li>\n                        <li class=\"item-wrap\">PRODUCTS</li>\n                        <li class=\"item-wrap\">GAMERS</li>\n                        <li class=\"item-wrap\">CONTACTS</li>\n                        <li class=\"item-wrap\">EVENTS</li>\n                    </ul>\n                </div>\n                <div class=\"soc-wrapper\" mbr-if=\"showSoc\">\n                    <div class=\"social-row\">\n                        <div class=\"soc-item\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon socicon-youtube\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"soc > 1\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon socicon-twitch\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"soc > 2\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon socicon-discord\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"soc > 3\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon socicon-twitter\"></span>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"copy-wrapper\" mbr-if=\"showCopy\">\n                    <p class=\"mbr-fonts-style copyright\" mbr-theme-style=\"display-7\" data-app-selector=\".copyright\" data-app-placeholder=\"Type Text\">\n                        © Copyright 2030 - All Rights Reserved\n                    </p>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            "& when (@fullWidth)": {
              ".container-fluid": {
                "padding": "0 120px",
                "@media (max-width: 768px)": {
                  "padding": "0 26px"
                }
              }
            },
            ".container": {
              "@media (max-width: 768px)": {
                "padding": "0 26px"
              }
            },
            ".row": {
              "padding": "60px",
              "margin": "0",
              "& when (@showBorder)": {
                "border": "1px solid @border",
                "padding": "120px 60px 60px 30px",
                "@media (max-width: 992px)": {
                  "padding": "120px 30px"
                }
              },
              "@media (max-width: 992px)": {
                "padding": "26px"
              }
            },
            ".title-wrapper": {
              "margin-bottom": "50px",
              ".title-wrap": {
                "display": "inline-flex",
                "align-items": "center",
                "@media (max-width: 992px)": {
                  "display": "block"
                },
                ".mbr-section-title": {
                  "margin-bottom": "0"
                },
                ".logo-wrap": {
                  "margin": "0 16px",
                  "img": {
                    "width": "110px",
                    "height": "100px",
                    "object-fit": "contain",
                    "@media (max-width: 992px)": {
                      "margin": "0 auto"
                    }
                  }
                }
              }
            },
            ".nav-wrapper": {
              "margin-bottom": "50px",
              ".list": {
                "display": "inline-flex",
                "justify-content": "center",
                "padding": "0",
                "margin": "0",
                "list-style-type": "none",
                "@media (max-width: 992px)": {
                  "display": "block"
                },
                ".item-wrap": {
                  "margin-right": "56px",
                  "margin-bottom": "0",
                  "padding-bottom": "2px",
                  "position": "relative",
                  "@media (max-width: 992px)": {
                    "margin": "0"
                  },
                  "&:hover": {
                    "&::before": {
                      "width": "100%"
                    }
                  },
                  "&::before": {
                    "content": "''",
                    "position": "absolute",
                    "bottom": "0",
                    "left": "0",
                    "height": "5px",
                    "width": "0",
                    "background-color": "@active",
                    "transition": "all 0.3s ease-in-out"
                  },
                  "&:last-child": {
                    "margin-right": "0"
                  }
                }
              }
            },
            ".soc-wrapper": {
              "margin-bottom": "50px",
              ".social-row": {
                "display": "inline-flex",
                ".soc-item": {
                  "margin-left": "24px",
                  "&:first-child": {
                    "margin-left": "0"
                  },
                  "a": {
                    "&:hover": {
                      ".mbr-iconfont": {
                        "display": "inline-flex",
                        "transform": "translateY(-5px)"
                      }
                    },
                    ".mbr-iconfont": {
                      "font-size": "22px",
                      "transition": "all 0.3s ease-in-out",
                      "color": "@socColor"
                    }
                  }
                }
              }
            },
            ".copy-wrapper": {
              ".copyright": {
                "margin-bottom": "0"
              }
            },
            ".mbr-section-title, .title-wrapper": {
              "color": "#d91717",
              "text-align": "center"
            },
            ".list": {
              "color": "#ffffff"
            },
            ".copyright": {
              "color": "#cacaca",
              "text-align": "center"
            },
            ".list, .nav-wrapper, .soc-wrapper": {
              "text-align": "center"
            }
          },
          "_cid": "tHZ1iXZ7gn",
          "_protectedParams": [],
          "_global": true,
          "_once": "footers",
          "_params": {},
          "_anchor": "footer1-t"
        }
      ]
    }
  }
}